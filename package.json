    "typeorm": "^0.2.25",
    "typescript": "^3.8.3",
    "uuid": "^8.0.0"
  },
  "scripts": {
    "start": "nodemon src/index.ts",
    "debug": "node --inspect-brk -r ts-node/register src/index.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "start-prod": "node build/index.js",
    "eslint": "eslint ./src --ext .jsx,.ts,.tsx,.js --cache",
    "eslint:fix": "yarn eslint --fix",
    "prettier": "prettier \"src/**/*\"",
    "prettier:fix": "yarn prettier --check",
    "lint": "yarn eslint && yarn prettier --list-different",
    "lint-fix": "yarn eslint:fix && yarn prettier:fix"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn type-check && yarn lint"
    }
  }
}
