  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/decay": "^1.0.0",
    "@types/express": "^4.17.6",
    "@types/express-rate-limit": "^5.0.0",
    "@types/hapi__joi": "^17.1.0",
    "@types/lodash": "^4.14.152",
    "@types/ms": "^0.7.31",
    "@types/node": "^13.13.5",
    "@types/rate-limit-redis": "^1.7.1",
    "@types/redis": "^2.8.20",
    "@types/uuid": "^8.0.0",
    "await-to-js": "^2.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "ms": "^2.1.2",
    "pg": "^8.1.0",
