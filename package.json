{
  "name": "retb",
  "description": "Razzle, Express and typeorm boilerplate",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "Yasin TAZEOGLU",
    "url": "https://github.com/yasintz"
  },
  "repository": {
    "url": "https://github.com/yasintz/retb.git"
  },
  "bugs": {
    "url": "https://github.com/yasintz/retb/issues"
  },
  "scripts": {
    "dev": "razzle start",
    "debug": "razzle start --inspect --inspect-port 9230",
    "build": "razzle build",
    "start": "node build/server.js",
    "clean": "rm -rf build",
    "lint": "eslint './src/**/*.ts*' '**/__tests__/**/*.test.ts*' --cache",
    "prettier": "prettier  --ignore-path ./.prettierignore  **/*.ts*",
    "fix": "yarn prettier --write && yarn lint --fix",
    "test": "CI=true razzle test --env=jsdom",
    "coverage": "yarn test --coverage",
    "check-rules": "yarn tsc && yarn lint && yarn prettier && yarn test",
    "now-build": "yarn build",
    "now-start": "cross-env NODE_ENV=production yarn start"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.4",
    "@jaredpalmer/after": "^1.5.0",
    "await-to-js": "^2.1.1",
    "axios": "^0.19.0",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "node-cache": "^5.0.2",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.18.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "serialize-javascript": "^3.0.0",
    "socket.io": "^2.3.0",
    "styled-components": "^5.0.1",
    "typeorm": "^0.2.20",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/classnames": "^2.2.9",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-rate-limit": "^5.0.0",
    "@types/express-session": "^1.17.0",
    "@types/jest": "^24.0.23",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.14",
    "@types/node-cache": "^4.2.5",
    "@types/passport": "^1.0.2",
    "@types/passport-github": "^1.1.5",
    "@types/passport-google-oauth20": "^2.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/pg": "^7.14.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router-dom": "^5.1.3",
    "@types/reflect-metadata": "^0.1.0",
    "@types/serialize-javascript": "^1.5.0",
    "@types/socket.io": "^2.1.4",
    "@types/styled-components": "^5.0.1",
    "@types/uuid": "^7.0.0",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^4.2.3",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "razzle": "^3.0.0",
    "razzle-heroku": "^3.0.0",
    "razzle-plugin-typescript": "^3.0.0",
    "svg-react-loader": "^0.4.6",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run check-rules"
    }
  },
  "engines": {
    "node": "12.16.1",
    "yarn": "1.22.4"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/**/__tests__/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  }
}
